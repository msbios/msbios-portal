{
  "name": "msbios/portal",
  "description": "MSBios Portal",
  "license": "GPL",
  "keywords": [
    "php"
  ],
  "homepage": "https://github.com/msbios/msbios-portal",
  "minimum-stability": "stable",
  "authors": [
    {
      "name": "Judzhin Miles",
      "email": "info@msbios.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "MSBios\\Portal\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MSBiosTest\\Portal\\": "tests/"
    }
  },
  "require": {
    "zendframework/zend-loader": "^2.5",
    "zendframework/zend-servicemanager": "^3.3",
    "zendframework/zend-modulemanager": "^2.8",
    "msbios/msbios": "^0.0.1",
    "msbios/resource": "^0.0.1",
    "msbios/authentication": "^0.0.3",
    "msbios/guard": "^0.0.16",
    "msbios/guard-resource": "^0.0.6",
    "msbios/guard-cpanel": "^0.0.3",
    "msbios/cpanel": "^0.0.8"
  },

  "require-dev": {
    "phpunit/phpunit": "^5.7",
    "zendframework/zend-test": "^3.1",
    "squizlabs/php_codesniffer": "^3.0",
    "zendframework/zend-mvc": "^3.1",
    "msbios/application": "^0.1.5",
    "msbios/debug": "^0.0.3"
  },
  "scripts": {
    "cs-check": "phpcs",
    "cs-fix": "phpcbf",
    "test": [
      "@cs-check",
      "phpunit"
    ],
    "alter": "./vendor/bin/doctrine-module orm:schema-tool:update --force",
    "serve": "php -S 0.0.0.0:3107 -t bin bin/msbios-portal-module.php",
    "commit": "git add . && git commit -a -m \"Lazy Composer Commiter\" && git push"
  }
}
